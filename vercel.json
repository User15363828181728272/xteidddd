{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },

    { "src": "^/$", "dest": "/index.js" },
    { "src": "^/docs$", "dest": "/index.js" },
    { "src": "^/dev$", "dest": "/index.js" },
    { "src": "^/nt$", "dest": "/index.js" },

    { "src": "/(.*)", "dest": "/api-page/nt.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}